	<!-- 채번 가져오기 -->
	<select id="makeChaebun" parameterType="ssm.cd.vo.TestVO" resultType="int">
		SELECT NVL(MAX(SUBSTR(CD_NO,-4)),0)+1 AS CD_NO FROM CAREER_DESIGN
	</select>
	
	<select id="getSs_grade" parameterType="ssm.cd.vo.TestVO" resultType="String">
		SELECT SS_GRADE FROM S_MEMBER WHERE SS_NO=#{ssNo}
	</select>
	
	<!-- 결과 저장하기 -->
	<insert id="insertResult" parameterType="ssm.cd.vo.TestVO">
	INSERT INTO CAREER_DESIGN(CD_NO,
							  SS_NO,
							  CD_GRADE,
							  CD_VALUE,
							  CD_URL,
							  CD_INSERTDATE,
							  CD_DELETEYN)
	VALUES                   (#{cdNo},
						      #{ssNo},
						      #{cdGrade},
						      #{cdValue},
						      #{cdUrl},
						      SYSDATE,
						      'Y') 
						      
	</insert>

	<!-- 학교정보 List Select -->	
	<select id="schoolInfoList" parameterType="ssm.cd.vo.School_DataVO" resultMap="SDVO" >
		SELECT
				#{sd_listSize} AS LISTSIZE,
				B.*
		FROM
		(
			SELECT
			  A.SD_NO,
		      A.SD_CAMPUSNAME,
		      A.SD_COLLAGEINFOURL,
		      A.SD_SCHOOLTYPE,
		      A.SD_LINK,
		      A.SD_SCHOOLGUBUN,
		      A.SD_ADRES,
		      A.SD_REGION,
		      A.SD_TOTALCOUNT,
		      A.SD_ESTTYPE,
		      A.SD_SCHOOLNAME,
		      A.SD_SEQ,
		      CEIL(ROW_NUMBER() OVER(ORDER BY A.SD_NO)/#{sd_listSize}) PAGENO,
		      ROW_NUMBER() OVER(ORDER BY A.SD_NO ASC) AS ROWNUMM,
		      COUNT(A.SD_NO) OVER() AS TOTALCOUNT
			FROM SCHOOL_DATA A
			WHERE 1=1
				<if test="sd_SchoolType != null and sd_SchoolType != '' and sd_SchoolType != 'all' ">
					AND A.SD_SCHOOLTYPE = #{sd_SchoolType}
				</if>
				<if test="go != '' and go != null ">
					AND A.SD_REGION IN (
					<trim suffixOverrides=",">
					<if test="region1 != null and region1 != '' ">
						#{region1},
					</if>
					<if test="region2 != null and region2 != '' ">
						#{region2},
					</if>
					<if test="region3 != null and region3 != '' ">
						#{region3},
					</if>
					<if test="region4 != null and region4 != '' ">
						#{region4},
					</if>
					<if test="region5 != null and region5 != '' ">
						#{region5},
					</if>
					<if test="region6 != null and region6 != '' ">
						#{region6},
					</if>
					<if test="region7 != null and region7 != '' ">
						#{region7},
					</if>
					<if test="region8 != null and region8 != '' ">
						#{region8},
					</if>
					<if test="region9 != null and region9 != '' ">
						#{region9},
					</if>
					<if test="region10 != null and region10 != '' ">
						#{region10},
					</if>
					<if test="region11 != null and region11 != '' ">
						#{region11},
					</if>
					<if test="region12 != null and region12 != '' ">
						#{region12},
					</if>
					<if test=" region13 != null and region13 != '' ">
						#{region13},
					</if>
					<if test="region14 != null and region14 != '' ">
						#{region14},
					</if>
					<if test="region15 != null and region15 != '' ">
						#{region15},
					</if>
					<if test="region16 != null and region16 != '' ">
						#{region16},
					</if>
					<if test="region17 != null and region17 != '' ">
						#{region17},
					</if>
					<if test="region18 != null and region18 != '' ">
						#{region18},
					</if>
					</trim>
					)
				</if>
				<if test="go2 != '' and go2 != null">
					AND A.SD_ESTTYPE IN (
						<trim suffixOverrides=",">
							<if test="sd_EstType1 != null and sd_EstType1 != '' ">
								#{sd_EstType1},
							</if>
							<if test="sd_EstType2 != null and sd_EstType2 != '' ">
								#{sd_EstType2},
							</if>
							<if test="sd_EstType3 != null and sd_EstType3 != '' ">
								#{sd_EstType3},
							</if>
						</trim>
					)
				</if>
		)B
		WHERE B.PAGENO = #{sd_curPage}
	</select>
	
	<!-- 학과정보 List SELECT -->
	<select id="majorList" parameterType="ssm.cd.vo.Major_DataVO" resultMap="MDVO">
	SELECT
				B.*
		FROM
		(
			SELECT
			  A.MDNO,
		      A.LCLASS,
		      A.FACILNAME,
		      A.MAJORSEQ,
		      A.MCLASS,
		      A.TOTALCOUNT,
		      CEIL(ROW_NUMBER() OVER(ORDER BY A.MDNO)/#{listSize}) PAGENO,
		      ROW_NUMBER() OVER(ORDER BY A.MDNO ASC) AS ROWNUMM,
		      COUNT(A.MDNO) OVER() AS RESCOUNT
			FROM MAJOR_DATA A
			WHERE 1=1
			<if test="go != '' and go != null ">
			AND A.LCLASS IN(
			<trim suffixOverrides=",">
			<if test="lClass2 != '' and lClass2 != null">
				#{lClass2},
			</if>
			<if test="lClass3 != '' and lClass3 != null">
				#{lClass3},
			</if>
			<if test="lClass4 != '' and lClass4 != null">
				#{lClass4},
			</if>
			<if test="lClass5 != '' and lClass5 != null">
				#{lClass5},
			</if>
			<if test="lClass6 != '' and lClass6 != null">
				#{lClass6},
			</if>
			<if test="lClass7 != '' and lClass7 != null">
				#{lClass7},
			</if>
			<if test="lClass8 != '' and lClass8 != null">
				#{lClass8},
			</if>
			</trim>
			) 
			</if>
		)B
		WHERE B.PAGENO = #{curPage}
	</select>
	
	<!-- detail selectOne -->
	<select id="majorDetail" parameterType="ssm.cd.vo.Major_DataVO" resultMap="mDetail">
		SELECT
			  A.LCLASS,
		      A.FACILNAME,
		      A.MAJORSEQ,
		      A.MCLASS,
		      A.TOTALCOUNT
		FROM MAJOR_DATA A
		WHERE A.MAJORSEQ = #{majorSeq}
	</select>
	
	<!-- jobList -->
	<select id="jobList" parameterType="ssm.cd.vo.Job_DataVO" resultMap="jdvo">
		SELECT
				
				<trim suffixOverrides=",">
					B.*,
					<if test="go != null and go != ''">
						#{go,jdbcType=VARCHAR} AS GO,
					</if>
					<if test="go1 != null and go1 != ''">
						#{go1,jdbcType=VARCHAR} AS GO1,
					</if>
					<if test="go2 != null and go2 != ''">
						#{go2,jdbcType=VARCHAR} AS GO2,
					</if>
				</trim>
				
				
		FROM
		(
			SELECT
			  A.JDNO,
		      A.PROFESSION,
		      A.SUMMARY,
		      A.SIMILARJOB,
		      A.SALARY,
		      A.JOBDICSEQ,
		      A.EQUALEMPLOYMENT,
		      A.TOTALCOUNT,
		      A.APTD_TYPE_CODE,
		      A.PROSPECT,
		      A.JOB_CTG_CODE,
		      A.JOB_CODE,
		      A.JOB,
		      A.POSSIBILITY,
		      CEIL(ROW_NUMBER() OVER(ORDER BY A.JDNO)/#{listSize}) PAGENO,
		      ROW_NUMBER() OVER(ORDER BY A.JDNO ASC) AS ROWNUMM,
		      COUNT(A.JDNO) OVER() AS RESCOUNT
			FROM JOB_DATA A
			WHERE 1=1
			<if test="go1 != '' and go1 != null">
				<if test="ssalary != null and ssalary != '' ">
				AND A.SALARY = #{ssalary}
				</if>
				<if test="pprospect != null and pprospect != '' ">
				AND A.PROSPECT = #{pprospect}
				</if>
				<if test="ppossibility != null and ppossibility != '' ">
				AND A.POSSIBILITY = #{ppossibility}
				</if>
				<if test="eequalemployment != null and eequalemployment != '' ">
