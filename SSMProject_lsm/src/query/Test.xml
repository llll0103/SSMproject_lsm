						<if test="JCCode11 != null and JCCode11 != '' ">#{JCCode11},</if>
						<if test="JCCode12 != null and JCCode12 != '' ">#{JCCode12},</if>
						<if test="JCCode13 != null and JCCode13 != '' ">#{JCCode13},</if>
						<if test="JCCode14 != null and JCCode14 != '' ">#{JCCode14},</if>
						<if test="JCCode15 != null and JCCode15 != '' ">#{JCCode15},</if>
						<if test="JCCode16 != null and JCCode16 != '' ">#{JCCode16},</if>
